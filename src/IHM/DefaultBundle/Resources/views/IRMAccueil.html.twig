{# empty Twig template #}
{% extends 'IRMbase.html.twig' %}
{% block graphAccueil %}
<script src="{{ asset('bundles/grdfdefault/js/SpryAssets/SpryMenuBar.js') }}" type="text/javascript"></script>
<link href="{{ asset('bundles/grdfdefault/js/SpryAssets/SpryMenuBarHorizontal.css') }}" rel="stylesheet" type="text/css">
<script type="text/javascript" src="{{ asset('bundles/grdfdefault/js/Chart.js-master/Chart.js') }}"></script>
	<script>

		var doughnutData = [
				{
					value: 300,
					color:"#F7464A",
					highlight: "#FF5A5E",
					label: "Red"
				},
				{
					value: 50,
					color: "#46BFBD",
					highlight: "#5AD3D1",
					label: "Green"
				},
				{
					value: 100,
					color: "#FDB45C",
					highlight: "#FFC870",
					label: "Yellow"
				},
				{
					value: 40,
					color: "#949FB1",
					highlight: "#A8B3C5",
					label: "Grey"
				},
				{
					value: 120,
					color: "#4D5360",
					highlight: "#616774",
					label: "Dark Grey"
				}

			];

			window.onload = function(){
				var ctx = document.getElementById("ns_mp-chart-area").getContext("2d");
				window.myDoughnut = new Chart(ctx).Doughnut(doughnutData, {responsive : true});
			};



	</script>
        
{% endblock %}

{% block graphChartArea %}
<div id="apDiv2"> <canvas id="ns_mp-chart-area" width="500" height="500"/> </canvas></div>
{% endblock %}